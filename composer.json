{
    "name": "rattanak-chuon/unit-conversions",
    "description": "Unit conversions",
    "keywords": [
        "rattanak.chuon",
        "unit-conversions"
    ],
    "homepage": "https://github.com/rattanak-chuon/unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Rattanak Chuon",
            "email": "rattanak.chuon@codingate.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Rattanak\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rattanak\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
